{
  "name": "blueprism",
  "version": "1.0.0",
  "description": "Integrates with Blueprism",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz blueprism.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run transpile && npm run lint && npm run zip",
    "lint": "tslint -c tslint.json src/**/*.ts"
  },
  "keywords": [
    "Robot Process Automation (RPA)",
    "Blueprism"
  ],
  "author": "Cognigy GmbH",
  "license": "MIT",
  "dependencies": {
    "@cognigy/extension-tools": "^0.10.0",
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.7",
    "@types/node": "^13.13.15",
    "@types/node-fetch": "^2.5.7",
    "typescript": "^3.8.3"
  }
}
