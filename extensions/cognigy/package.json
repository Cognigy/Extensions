    "@types/html-to-text": "9.0.4",
    "html-to-text": "9.0.5",
    "jsdom": "24.1.3",
    "langchain": "0.3.31",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/jsdom": "21.1.7",
    "@types/node": "13.13.52",
    "typescript": "5.9.2",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.32.0"
  }
