    "Conversational AI"
  ],
  "author": "Cognigy GmbH",
  "license": "MIT",
  "dependencies": {
    "@cognigy/extension-tools": "^0.13.0",
    "axios": "^0.21.2",
    "juice": "^7.0.0",
    "moment": "^2.29.2",
    "nodemailer": "^6.4.16",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^13.13.15",
    "@types/node-fetch": "^2.5.7",
    "typescript": "^3.8.3"
  }
}
