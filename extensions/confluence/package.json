{
    "name": "confluence",
    "version": "5.0.0",
    "description": "Integrates with the Atlassian's Confluence.",
    "main": "build/module.js",
    "scripts": {
        "transpile": "tsc -p .",
        "zip": "tar cfz confluence.tar.gz build/* package.json package-lock.json README.md icon.png",
        "build": "npm run transpile && npm run lint && npm run zip",
        "lint": "eslint \"src/**/*.ts\""
    },
    "keywords": [
        "Knowledge",
        "Wiki",
        "confluence-kai",
        "extensions"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Cognigy/Extensions"
    },
    "author": "Cognigy GmbH",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^24.2.0",
        "@types/turndown": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^8.15.0",
        "@typescript-eslint/parser": "^8.15.0",
        "eslint": "^9.32.0",
        "typescript": "^5.9.2"
    },
    "dependencies": {
        "@cognigy/extension-tools": "0.16.5-rc3",
        "@langchain/textsplitters": "^0.0.3",
        "axios": "^1.11.0",
        "turndown": "^7.1.2"
    }
}