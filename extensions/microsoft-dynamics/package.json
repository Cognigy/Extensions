{
  "name": "microsoft-dynamics",
  "version": "4.0.0",
  "description": "Integrates with Microsoft Dynamics 365",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz microsoft-dynamics.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run transpile && npm run lint && npm run zip",
    "lint": "tslint -c tslint.json src/**/*.ts"
  },
  "keywords": [
    "Microsoft",
    "CRM",
    "Contacts"
  ],
  "author": "Cognigy GmbH",
  "license": "MIT",
  "dependencies": {
    "@cognigy/extension-tools": "^0.10.0",
    "axios": "^0.21.1",
    "qs": "^6.10.1",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@types/node": "^13.13.15",
    "@types/qs": "^6.9.6",
    "typescript": "^3.8.3"
  }
}
