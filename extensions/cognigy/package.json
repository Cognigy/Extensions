{
  "name": "cognigy",
  "version": "5.0.0",
  "description": "Basic Cognigy Extensions",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz cognigy.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run transpile && npm run lint && npm run zip",
    "lint": "eslint \"src/**/*.ts\""
  },
  "keywords": [
    "Cognigy",
    "Conversational AI",
    "knowledge extensions"
  ],
  "author": "Cognigy GmbH",
  "license": "MIT",
  "dependencies": {
    "@cognigy/extension-tools": "0.16.5-rc5",
    "@dqbd/tiktoken": "1.0.7",
    "@types/html-to-text": "9.0.4",
    "html-to-text": "9.0.5",
    "jsdom": "24.1.3",
    "langchain": "0.3.31"
  },
  "devDependencies": {
    "@eslint/js": "9.34.0",
    "@types/jsdom": "21.1.7",
    "@types/node": "13.13.52",
    "eslint": "9.34.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.41.0"
  }
}
