{
  "name": "audiocodes",
  "version": "4.0.0",
  "description": "Get Call Information from Audiocodes",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz audiocodes.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run transpile && npm run lint && npm run zip",
    "lint": "tslint -c tslint.json src/**/*.ts"
  },
  "keywords": [
    "Call",
    "Audiocodes",
    "Recording"
  ],
  "author": "Cognigy GmbH",
  "license": "MIT",
  "dependencies": {
    "@cognigy/extension-tools": "^0.10.0",
    "axios": "^0.21.1",
    "https": "^1.0.0",
    "nodemailer": "^6.4.17",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^13.13.15",
    "@types/nodemailer": "^6.4.0",
    "typescript": "^3.8.3"
  }
}
