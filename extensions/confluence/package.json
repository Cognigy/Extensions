{
    "name": "confluence",
    "version": "5.0.0",
    "description": "Integrates with the Atlassian's Confluence.",
    "main": "build/module.js",
    "scripts": {
        "transpile": "tsc -p .",
        "zip": "tar cfz confluence.tar.gz build/* package.json package-lock.json README.md icon.png",
        "build": "npm run transpile && npm run lint && npm run zip",
        "format": "biome check --write",
        "lint": "biome check"
    },
    "keywords": [
        "Knowledge",
        "Wiki",
        "confluence-kai",
        "extensions"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Cognigy/Extensions"
    },
    "author": "Cognigy GmbH",
    "license": "MIT",
    "devDependencies": {
        "@biomejs/biome": "2.2.4",
        "@types/node": "^24.2.0",
        "@types/turndown": "^5.0.1",
        "typescript": "5.9.2"
    },
    "dependencies": {
        "@cognigy/extension-tools": "0.16.5-rc5",
        "@langchain/textsplitters": "^0.0.3",
        "turndown": "^7.1.2"
    }
}
