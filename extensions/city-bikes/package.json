    "license": "MIT",
    "dependencies": {
        "@cognigy/extension-tools": "^0.9.0",
        "axios": "^0.19.2",
        "tslint": "^6.1.2"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/node": "^13.13.15",
        "@types/node-fetch": "^2.5.7",
        "typescript": "^3.8.3"
    }
}