{
  "name": "bmc",
  "version": "4.0.0",
  "description": "Integrates with BMC",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz bmc.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run transpile && npm run lint && npm run zip",
    "lint": "tslint -c tslint.json src/**/*.ts"
  },
  "keywords": [
    "Twilio",
    "SMS"
  ],
  "author": "Cognigy GmbH",
  "license": "MIT",
  "dependencies": {
    "@cognigy/extension-tools": "^0.10.0",
    "axios": "^0.20.0",
    "tslint": "^6.1.2",
    "twilio": "^3.33.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^13.13.15",
    "@types/twilio": "0.0.10",
    "typescript": "^3.8.3"
  }
}
