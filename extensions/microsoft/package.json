	"license": "MIT",
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/node": "^10.17.28",
		"tslint": "^5.16.0",
		"typescript": "^3.1.6"
	},
	"dependencies": {
		"@cognigy/extension-tools": "^0.10.0",
		"@microsoft/microsoft-graph-client": "^1.7.0",
		"axios": "^0.20.0",
		"request": "^2.88.2",
		"request-promise": "^4.2.6"
	}
