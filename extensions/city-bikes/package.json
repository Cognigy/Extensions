{
    "name": "city-bikes",
    "version": "1.0.0",
    "description": "Find nearest bike station.",
    "main": "build/module.js",
    "scripts": {
        "transpile": "tsc -p .",
        "zip": "tar cfz city-bikes.tar.gz build/* package.json package-lock.json README.md icon.png",
        "build": "npm run transpile && npm run lint && npm run zip",
        "lint": "tslint -c tslint.json src/**/*.ts"
