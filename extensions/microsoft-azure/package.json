    "@types/node": "^10.14.6",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@cognigy/extension-tools": "^0.11.0",
    "axios": "^0.19.2",
    "uuid": "3.3.2"
  }
}
