{
  "name": "bamboo-hr",
  "version": "4.0.0",
  "description": "Integrates with Bamboo HR",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz bamboohr.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run transpile && npm run lint && npm run zip",
    "lint": "tslint -c tslint.json src/**/*.ts"
  },
  "keywords": [
    "Bamboo HR",
    "Human Resources"
  ],
  "author": "Cognigy GmbH",
  "license": "MIT",
  "dependencies": {
    "@cognigy/extension-tools": "^0.10.0",
    "axios": "^0.21.1",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^13.13.15",
    "@types/node-fetch": "^2.5.7",
    "typescript": "^3.8.3"
  }
}
