    "Tyntec",
    "SMS"
  ],
  "author": "Cognigy GmbH",
  "license": "MIT",
  "dependencies": {
    "@cognigy/extension-tools": "^0.13.0",
    "axios": "^1.6.0",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@types/node": "^13.13.15",
    "typescript": "^3.8.3"
  }
}
