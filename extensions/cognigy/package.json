{
  "name": "cognigy",
  "version": "4.2.0",
  "description": "Basic Cognigy Extensions",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz cognigy.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run transpile && npm run lint && npm run zip && npm run playwright:install:chromium",
    "lint": "tslint -c tslint.json src/**/*.ts -e node_modules",
    "playwright:install:chromium": "npx playwright@1.51.0 install chromium --no-shell"
  },
  "keywords": [
    "Cognigy",
    "Conversational AI"
  ],
  "author": "Cognigy GmbH",
  "license": "MIT",
  "dependencies": {
    "@cognigy/extension-tools": "0.16.5-rc3",
    "@dqbd/tiktoken": "1.0.7",
    "axios": "^0.21.2",
    "juice": "^7.0.0",
    "langchain": "0.0.141",
    "moment": "^2.29.2",
    "nodemailer": "^6.4.16",
    "playwright": "1.51.0",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "13.13.52",
    "@types/node-fetch": "^2.5.7",
    "typescript": "5.9.2"
  }
}
