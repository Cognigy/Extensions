{
  "name": "diffbot",
  "version": "2.0.0",
  "description": "Integrates with the Diffbot API",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p tsconfig.production.json",
    "zip": "tar cfz diffbot.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run test && npm run transpile && npm run lint && npm run zip",
    "format": "biome check --write",
    "lint": "biome check",
    "test": "node --test --import=tsx --experimental-test-coverage --test-coverage-exclude='**/*.test.ts'"
  },
  "keywords": [
    "Diffbot",
    "Conversational AI",
    "knowledge extensions"
  ],
  "engines": {
      "node": ">=22.22.0",
      "npm": ">=11.9.0"
  },
  "author": "Cognigy GmbH",
  "license": "MIT",
  "dependencies": {
    "@cognigy/extension-tools": "^0.17.0-rc4",
    "fetch-retry": "6.0.0",
    "flattie": "1.1.1",
    "json-to-plain-text": "1.2.0",
    "langchain": "0.3.31"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@types/node": "24.2.0",
    "tsx": "4.21.0",
    "typescript": "5.9.2"
  }
}
