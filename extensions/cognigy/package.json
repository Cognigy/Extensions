{
  "name": "cognigy",
  "version": "4.3.0",
  "description": "Basic Cognigy Extensions",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz cognigy.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run transpile && npm run lint && npm run zip",
    "lint": "tslint -c tslint.json src/**/*.ts"
  },
  "keywords": [
    "Cognigy",
    "Conversational AI"
  ],
  "author": "Cognigy GmbH",
  "license": "MIT",
  "dependencies": {
    "@cognigy/extension-tools": "0.16.5-rc3",
    "@dqbd/tiktoken": "1.0.7",
    "@types/html-to-text": "9.0.4",
    "html-to-text": "9.0.5",
    "jsdom": "22.1.0",
    "langchain": "0.0.141",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jsdom": "21.1.7",
    "@types/node": "13.13.52",
    "typescript": "5.9.2"
  }
}
