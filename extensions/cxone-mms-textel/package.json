{
  "name": "textel-mms",
  "version": "1.0.1",
  "description": "Send SMS/MMS via Textel",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz textel-mms.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run transpile && npm run lint && npm run zip",
    "lint": "tslint -c tslint.json src/**/*.ts"
  },
  "keywords": [
    "Textel",
    "MMS",
    "SMS"
  ],
  "author": "NiCE Alexei",
  "license": "NiCE",
  "dependencies": {
    "@cognigy/extension-tools": "^0.14.0",
    "jsonwebtoken": "^9.0.2",
    "tslint": "^6.1.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "typescript": "^5.9.2"
  }
}
