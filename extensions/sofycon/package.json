{
  "name": "sofycon",
  "version": "4.0.0",
  "description": "Integrates with Sofycon",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz sofycon.tar.gz build/* package.json package-lock.json README.md icon.png",
