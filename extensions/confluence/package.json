{
    "name": "confluence",
    "version": "5.1.0-rc.0",
    "description": "Integrates with the Atlassian's Confluence.",
    "main": "build/module.js",
    "scripts": {
        "transpile": "tsc -p .",
        "zip": "tar cfz confluence-$npm_package_version.tar.gz build/* package.json package-lock.json README.md icon.png",
        "build": "npm run transpile && npm run lint && npm run zip",
        "format": "biome check --write",
        "lint": "biome check",
        "test": "tsx --test src/**/*.test.ts"
    },
    "keywords": [
        "Knowledge",
        "Wiki",
        "confluence-kai",
        "extensions"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Cognigy/Extensions"
    },
    "author": "Cognigy GmbH",
    "license": "MIT",
    "devDependencies": {
        "@biomejs/biome": "2.2.4",
        "@types/node": "^24.2.0",
        "@types/turndown": "^5.0.1",
        "tsx": "4.21.0",
        "typescript": "5.9.2"
    },
    "dependencies": {
        "@cognigy/extension-tools": "^0.17.0-rc4",
        "@langchain/textsplitters": "0.1.0",
        "turndown": "7.2.2"
    }
}
