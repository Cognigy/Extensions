{
  "name": "tyntec",
  "version": "4.0.0",
  "description": "Exposes Tyntec functionality to Cognigy.AI",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz tyntec.tar.gz build/* package.json package-lock.json README.md icon.png",
